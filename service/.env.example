NODE_ENV=development
PORT=3000

# Neon pooled connection for runtime
DATABASE_URL=postgresql://USER:PASSWORD@EP-XXXX-pooler.ap-southeast-1.aws.neon.tech/DB_NAME?sslmode=require&pgbouncer=true&connect_timeout=15
# Neon direct connection for migration/maintenance
DIRECT_URL=postgresql://USER:PASSWORD@EP-XXXX.ap-southeast-1.aws.neon.tech/DB_NAME?sslmode=require

JWT_SECRET=replace_with_strong_secret
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=replace_with_strong_refresh_secret
JWT_REFRESH_EXPIRES_IN=30d

BLOB_READ_WRITE_TOKEN=vercel_blob_rw_xxx
AMAP_WEB_SERVICE_KEY=your_amap_web_service_key

# Comma-separated origins
CORS_ORIGINS=http://localhost:3001,https://your-netlify-site.netlify.app
